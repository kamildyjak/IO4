<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pl.io4.model.database.entities.TaxRule" table="TaxRule" schema="dbo" catalog="io4">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="symbol">
            <column name="symbol" sql-type="char"/>
        </property>
        <property name="percent">
            <column name="percent" sql-type="tinyint"/>
        </property>
    </class>
</hibernate-mapping>