<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pl.io4.model.database.entities.ShopEntity" table="Shop" schema="dbo" catalog="io4">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="name">
            <column name="name" sql-type="nchar(10)" length="10" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="nvarchar(max)" length="2147483647"/>
        </property>
    </class>
</hibernate-mapping>